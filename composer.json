{
  "name": "amrikasir/lang-selector",
  "description": "Livewire-based language selector for Laravel 11",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Al Amrikasir",
      "email": "amrikasir@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "LanguageSelector\\App\\": "src/app/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "LanguageSelector\\App\\Providers\\LangSelectorServiceProvider"
      ]
    }
  },
  "require": {
    "php": "^8.2",
    "illuminate/support": ">=11.0",
    "livewire/livewire": "^3.0",
    "vemcogroup/laravel-translation": "^3.0"
  },
  "minimum-stability": "dev"
}